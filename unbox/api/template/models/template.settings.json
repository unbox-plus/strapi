{
  "kind": "collectionType",
  "connection": "default",
  "collectionName": "templates",
  "info": {
    "name": "0. Lojas"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "footer": {
      "model": "footer-information"
    },
    "logo": {
      "model": "logo"
    },
    "texto_topo": {
      "model": "texto-topo"
    },
    "name": {
      "type": "string",
      "unique": true,
      "required": true,
      "private": true
    },
    "header_link": {
      "model": "header-links"
    },
    "carousel_top": {
      "model": "carousel-top"
    },
    "carousel_products": {
      "model": "carousel-products"
    },
    "buy_button": {
      "model": "buy-button"
    },
    "instagram_section": {
      "model": "instagram-section"
    },
    "categories_section": {
      "model": "categories-section"
    },
    "featured_product": {
      "model": "featured-product"
    },
    "feedbacks_section": {
      "model": "feedbacks"
    },
    "primaryColor": {
      "type": "string",
      "required": true,
      "maxLength": 7,
      "minLength": 7,
      "default": "#000000"
    },
    "secondaryColor": {
      "type": "string",
      "default": "#ffffff",
      "maxLength": 7,
      "minLength": 7,
      "required": true
    },
    "about_page": {
      "model": "about-page"
    },
    "terms_page": {
      "model": "terms-page"
    },
    "shopName": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "favicon": {
      "model": "favicon"
    },
    "SEO": {
      "type": "component",
      "repeatable": false,
      "component": "componentes.seo"
    },
    "Order": {
      "type": "component",
      "repeatable": true,
      "component": "componentes.order"
    },
    "backendUrl": {
      "type": "string"
    },
    "productsInHome": {
      "type": "integer",
      "min": 0,
      "max": 999
    },
    "productsInCategory": {
      "type": "integer",
      "max": 999,
      "min": 0
    },
    "idShop": {
      "type": "string"
    },
    "new_feedbacks_section": {
      "model": "newfeedbacks-section"
    },
    "new_featured_products": {
      "model": "newfeatured-products"
    },
    "slug": {
      "type": "string",
    }
  }
}
